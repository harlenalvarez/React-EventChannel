    "postpublish": "git checkout ./package.json"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react-swc": "3.0.0",
    "json": "11.0.0",
    "vite-plugin-dts": "1.7.1",
    "typescript": "4.9.4",
    "vite": "4.0.1"
  },
  "peerDependencies": {
    "react": ">=17.x",
    "react-dom": ">=17.x"
  }
}